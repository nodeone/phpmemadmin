{
  "name": "clickalicious/phpmemadmin",
  "description": "phpMemAdmin - Bringing Memcached to the web",
  "authors": [
    {
      "name": "Benjamin Carl",
      "email": "opensource@clickalicious.de",
      "homepage": "http://www.clickalicious.de",
      "role": "Developer"
    }
  ],
  "config":{
        "sort-packages": true,
        "platform": {
            "php": "5.6"
        }
  },
  "require": {
    "php": ">=5.4.0",
    "clickalicious/memcached.php": "^v1.0.1",
    "wp-cli/php-cli-tools": "~0.10"
  },
  "require-dev": {
    "satooshi/php-coveralls": "~1.0.1"
  },
  "autoload": {
    "psr-4": {
      "Clickalicious\\PhpMemAdmin\\": "lib/Clickalicious/PhpMemAdmin/"
    }
  },
  "include-path": ["lib/"],
  "minimum-stability": "dev",
  "keywords": [
    "Memcached",
    "Admin",
    "Client",
    "Cluster",
    "Management",
    "Pool",
    "PHP",
    "clickalicious",
    "phpMyAdmin"
  ],
  "homepage": "https://github.com/clickalicious/phpMemAdmin",
  "license": [
    "BSD-2.0"
  ],
  "support": {
    "email": "opensource@clickalicious.de",
    "issues": "https://github.com/clickalicious/phpMemAdmin/issues",
    "wiki": "https://github.com/clickalicious/phpMemAdmin/wiki"
  }
}
